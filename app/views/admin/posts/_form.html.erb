<%= simple_form_for(@admin_post,:url => @url ) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  <%= f.input :order%>
  <%= f.input :title%>
  <%= f.input :sub_title%>
   <%= f.input :published_at%>
  <%= f.input :description,as: :ckeditor%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
